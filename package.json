{
  "name": "@tapico/node-openfeature",
  "version": "0.0.1",
  "description": "OpenFeature provider for feature flags",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "test": "jest",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "format": "prettier",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "openfeature",
    "opentelemetry",
    "feature",
    "feature flag"
  ],
  "author": "Weyert de Boer",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.1",
    "jest-junit": "^13.2.0",
    "jest-serializer-path": "^0.1.15",
    "prettier": "^2.7.0",
    "ts-jest": "^28.0.5",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tapico/node-posthog-openfeature.git"
  },
  "dependencies": {
    "@openfeature/nodejs-sdk": "0.0.1-alpha.11",
    "@opentelemetry/api": ">=1.0.0",
    "@opentelemetry/api-metrics": ">=0.29.0",
    "posthog-node": "^1.3.0",
    "undici": "^5.5.1"
  },
  "peerDependencies": {
    "@opentelemetry/api": ">=1.0.0",
    "@opentelemetry/api-metrics": ">=0.29.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.16"
  },
  "volta": {
    "node": "16.15.1",
    "npm": "8.11.0"
  }
}
