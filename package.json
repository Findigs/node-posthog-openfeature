{
  "name": "@tapico/node-openfeature-posthog",
  "version": "0.9.2",
  "description": "OpenFeature provider for the PostHog feature flags solution",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "test": "jest",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "format": "prettier",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "openfeature",
    "opentelemetry",
    "feature",
    "feature flag"
  ],
  "author": "Weyert de Boer",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@opentelemetry/api-metrics": "0.33.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.0",
    "jest-junit": "^15.0.0",
    "jest-serializer-path": "^0.1.15",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tapico/node-posthog-openfeature.git"
  },
  "dependencies": {
    "@openfeature/js-sdk": "1.1.0",
    "@opentelemetry/api": ">=1.4.0",
    "posthog-node": "^2.2.3"
  },
  "peerDependencies": {
    "@opentelemetry/api": ">=1.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.16"
  },
  "volta": {
    "node": "16.15.1",
    "npm": "8.11.0"
  }
}
