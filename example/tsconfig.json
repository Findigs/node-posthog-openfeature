{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "lib": ["es2022", "dom"],
    "types": ["node"],
    "typeRoots": ["node_modules"],
    "outDir": "dist",
    "stripInternal": true,
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["index.ts"]
}
